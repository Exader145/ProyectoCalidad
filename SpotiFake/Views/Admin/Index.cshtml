@{
    Layout = "~/Views/Shared/_LayoutSys.cshtml";

 }
<br />
<form action="/Admin/FormularioAdmin" method="post">
    <button id="btnRegistrarAdministrador" class="btn-danger">Registrar Administrador</button>
</form>
<br />
<table class="table">
    <thead>
        <tr>
            <th>Nombres</th>
            <th>Correo Electronico</th>
            <th>Fecha de creacion de cuenta </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var Admin in Model)
        {
            <tr>

                <td>@Admin.nombre</td>
                <td>@Admin.correoElectronico</td>
                <td>@Admin.fechaCreación</td>
                <td>
                    <a id="btnEditar" href="/Admin/FormularioModificar?idUsuario=@Admin.idUsuario">EDITAR</a>|
                    <a id="btnEliminar" href="/Admin/eliminar?idUsuario=@Admin.idUsuario">ELIMINAR</a>
                </td>
            </tr>
        }
    </tbody>
</table>