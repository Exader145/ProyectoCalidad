
@{
    Layout = null;
}


@foreach (var detalleusuario in Model)
{
    <tr id="user">
        <td>@detalleusuario.usuario.nombre</td>
        <td>
            <form action="/contacto/dejarAmigo" method="post">
                <input name="idContacto" value="@detalleusuario.idContacto" style="display:none" />
                <button id="btnEliminarAmigo" name="idUsuario" value="@Session["IdUsuario"]" class="btn btn-primary">dejar de seguir </button>
            </form>
        </td>
    </tr>
}